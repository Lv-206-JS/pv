<div class="menu-toolbar">
    <% if (page !== 'projects') { %>
    <p class="show-project-name"> </p>
    <% } %>
    <div class="tool-bar">
        <label id="zoom-value" class="zoom-value">100%</label>
        <a class="tool-img tool-zoom-out" title="Zoom Out"></a>
        <a class="tool-img tool-zoom-in" title="Zoom In"></a>
        <a class="tool-img tool-undo hide-button" title="Undo" id="undo"></a>
        <a class="tool-img tool-redo hide-button" title="Redo" id="redo"></a>
        <a class="edit-milestone tool-img tool-milestone" title="Milestones"></a>
        <a class="tool-critical-way tool-img" title="Critical Path"></a>
        <a class="show-attachments tool-attachment tool-img" title="Attachments"></a>
        <a class="show-resources tool-resources tool-img" title="Resources"></a>
        <a class="show-ownership tool-ownership tool-img" title="Ownership"></a>
        <a class="show-settings tool-settings tool-img" title="Settings"></a>
    </div>
</div>
<div id="milestone-view-container"></div>
