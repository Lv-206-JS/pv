<div class="left-panel">
    <table class="tasks-list">
        <thead class="thead">
        <td class="td">Task<button class="add-task">Add New Task</button></td>
        <td class="td">Estimate</td>
        <td class="td">Depends On</td>
        <td class="td">Resource</td>
        </thead>
        <tbody class="tbody">
        <% for (var i = 0; i < tasks.length; i++) { %>
        <tr>
            <td class="td task-name" id="<%= tasks[i].taskId%>"><%= tasks[i].name%></td>
            <td class="td task-estimateTime"><%= tasks[i].estimateTime%></td>
            <% var dependencies = [];%>
            <% for (var j=0;j<tasks[i].dependsOn.length;j++) { %>
            <!--TO DO false id string in db-->
            <% if (tasks[i].dependsOn[j].taskId != "false") { %>
            <% dependencies.push(tasks[i].dependsOn[j].taskId); %>


            <% } %>
            <% } %>
            <td class="td task-dependsOn"><%= dependencies.toString() %></td>
            <td class="td task-resource"><%= tasks[i].resource%></td>
        </tr>
        <% } %>
        <% for (var i = 0; i <11; i++) { %>
        <tr>
            <td class="td task-name"></td>
            <td class="td task-estimateTime>"></td>
            <td class="td task-dependsOn"></td>
            <td class="td task-resource"></td>
        </tr>
        <% } %>
        </tbody>
    </table>
</div>
<div class="splitter"></div>
