
<div class="projects-list-header"><h3>Projects List</h3></div>

<ul>
    <% for(var i = 0; i < projects.length; i++) {%>
        <li class="projects-list-item <% if (projects[i].id === activeId) { %>active<% } %>" data-id="<%= projects[i].id %>">
            <a class="projects-link-name"> <%= projects[i].name %> </a>
            <a class="project-link icon-edit" title="Edit" data-id="<%= projects[i].id %>"></a>
            <a class="project-link icon-remove" title="Delete" data-id="<%= projects[i].id %>"></a>
            <a class="projects-go-link icon-open" title="Open" data-id="<%= projects[i].id %>"></a>
        </li>
    <% } %>
</ul>
