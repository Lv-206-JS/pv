<div class="landing-menu">
    <ul>
        <li><img class="logo" src="/images/plan-view-logo.svg" alt="logo"></li>
        <% if (userId) {%>
            <li><a class="go-to-projects main-menu-button">my projects</a></li>
            <div class="menu-right">
                <p class="show-user-name user-name">hello, <%= userName %> </p>
                <a class="sign-out-button main-menu-button">Sign Out</a>
            </div>
        <% } else { %>
            <li><a class="registration-btn main-menu-button">Sign Up</a></li>
            <li><a class="login-btn main-menu-button">Sign In</a></li>
        <% } %>
    </ul>
</div>